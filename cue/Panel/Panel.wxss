.list-box {
  padding: 0 20rpx;
}

.list-title {
  height: 56rpx;
  line-height: 56rpx;
  padding: 20rpx 0;
}

.list-title, .list-item {
  display: flex;
  justify-content: space-between;
  flex-direction: row;
}

.list-item {
  position: relative;
  padding: 20rpx 0;
}

.list-item::after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1px solid #eee;
  color: #eee;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.list-item-image {
  /* margin-right: 20rpx; */
  height: 180rpx;
}

/* .list-item-image{display: flex;justify-content: space-between; height:200rpx;flex-direction: column} */

