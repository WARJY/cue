.flex-row, .flex-column {
  display: flex;
  justify-content: space-between;
}

.flex-column {
  flex-direction: column;
}

.flex-row {
  flex-direction: row;
}

.row-1, .row-2, .row-3 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}

.row-1 {
  -webkit-line-clamp: 1;
  line-height: 1;
}

.row-2 {
  -webkit-line-clamp: 2;
  line-height: 1.5;
}

.row-3 {
  -webkit-line-clamp: 3;
  line-height: 1.5;
}

