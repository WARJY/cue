  display: flex;
  justify-content: space-between;
  flex-direction: column;
}

.none {
  display: none;
}

.mt10 {
  margin-top: 10rpx;
}

.mb10 {
  margin-bottom: 10rpx;
}

.plr20 {
  padding: 0 20rpx;
}

.bold {
  font-weight: 500;
}

.list-block-ft {
  position: relative;
  width: 50rpx;
  text-align: right;
  padding-right:4rpx;

}

.list-block-ft.show_arrow_top::after, .list-block-ft.show_arrow_middle::after {
  content: " ";
  display: inline-block;
  height: 16rpx;
  width: 16rpx;
  border-width: 1px 1px 0 0;
  border-color: #c8c8cd;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  /* position: absolute; */
  margin-top: 30rpx;
  right: 2px;
}

.list-block-ft.show_arrow_middle::after {
  margin-top: 60rpx;
}

