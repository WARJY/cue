.page {
  width: 100%;
  min-height: 100vh;
  background-color: #f5f5f5;
  color: #333;
}

.container {
  width: 100%;
  box-sizing: border-box;
  padding: 0 20rpx;
}

.flex {
  display: flex;
  justify-content: center;
  align-items: center;
}

.flex-item {
  flex: 1;
}

.page-cell {
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx;
  margin: 0 0 20rpx 0;
  background-color: #fff;
}

.panel {
  padding: 20rpx 30rpx;
}

.panel .title {
  font-size: 40rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}

.panel .content {
  font-size: 28rpx;
  margin-top: 10rpx;
}

.page-panel {
  width: 100%;
  box-sizing: border-box;
  border-top: solid 1px #eee;
  margin-top: 20rpx;
  padding: 20rpx 0rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.page-panel .content {
  flex: 1;
  margin-left: 20rpx;
}

.page-panel .title1 {
  font-size: 40rpx;
  font-weight: bold;
  align-items: left;
}

.page-panel .right1 {
  font-size: 28rpx;
  font-weight: normal;
  text-align: right;
  flex: 1;
}

.page-panel .title2 {
  font-size: 28rpx;
  margin-top: 20rpx;
  color: #999;
}

.page-cell .item {
  width: calc(100% - 80rpx);
  position: relative;
  margin: 0 40rpx;
  box-sizing: border-box;
  border-bottom: solid 1px #eee;
  height: 40rpx;
  margin-bottom: 40rpx;
  margin-top: 10rpx;
}

.page-cell .item .title {
  width: 100%;
  text-align: center;
  position: absolute;
  font-size: 40rpx;
  top: 10rpx;
  z-index: 2;
}

.page-cell .item .title>view {
  width: 50%;
  margin: 0 auto;
  background-color: #fff;
}

.reply-tip {
  width: 100%;
  box-sizing: border-box;
  background-color: #fcf8e3;
  padding: 20rpx 0;
  text-align: center;
  margin: 0 auto;
  margin-top: 60rpx;
  font-size: 32rpx;
  border:1px solid #f0c48b;
  border-radius: 10rpx;
}

.reply-tip .blue {
  color: #246ddd;
}

.contact image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.contact {
  padding: 0 30rpx;
  color: #666;
  font-size: 32rpx;
  margin-top: 10rpx;
}

.content-panel {
  padding: 0 10rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  margin-top: 20rpx;
}

.content-panel .title {
  font-weight: bold;
}

.content-panel .image {
  width: 100%;
  margin-top: 20rpx;
}

.content-panel .image image {
  width: 100%;
  height: auto;
  margin: 10rpx 0;
}

.comment {
  font-size: 28rpx;
  padding: 0 20rpx;
}

.comment .count {
  color: blue;
}

.comment-item {
  margin-top: 20rpx;
  align-items: flex-start;
  border-bottom: solid 1rpx #eee;
  padding-bottom: 20rpx;
}

.comment-item .user {
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
  margin-top: 10rpx;
}

.comment-item .icon {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
}

.comment-item .name {
  width: 100%;
  font-size: 30rpx;
}

.comment-item .date {
  color: #999;
}

.comment-item .content {
  color: #999;
  margin-top: 10rpx;
}

.comment-panel {
  width: 100%;
  box-sizing: border-box;
  height: 100rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  box-shadow: 0 0 10rpx #ccc;
  padding: 0 40rpx;
}

.comment-panel .icon {
  width: 40rpx;
  height: 40rpx;
  margin: 0 20rpx;
}

.comment-panel .input {
  font-size: 30rpx;
  padding-left: 20rpx;
  border-radius: 20rpx;
  border: solid 1rpx #eee;
  height: 50rpx;
  line-height: 50rpx;
  margin-right: 20rpx;
  color: #999;
}

.comment-panel .commit {
  font-size: 24rpx;
  margin-left: 20rpx;
  padding: 10rpx 40rpx;
  background-color: #00ae66;
  border-radius: 20rpx;
  color: #fff;
}

.comment-input {
  width: 100%;
  background-color: #fff;
  padding: 20rpx;
  box-sizing: border-box;
  position: fixed;
  top: 0rpx;
  z-index: 100;
  height: 100vh;
}

.comment-input textarea {
  box-sizing: border-box;
  border: solid 1rpx #eee;
  border-radius: 20rpx;
  padding: 20rpx;
  font-size: 28rpx;
  height: 200rpx;
  margin: 0;
}

.comment-input .commit-box {
  width: 100%;
  text-align: right;
  margin: 20rpx 0;
}

.comment-input .commit {
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  background-color: #00ae66;
  font-size: 28rpx;
  border-radius: 20rpx;
  text-align: center;
  display: inline-block;
}

.quick-reply .title {
  font-size: 30rpx;
  color: #000;
}

.quick-reply .reply-box {
  margin: 20rpx 0;
  width: 100%;
}

.reply-box .item {
  display: inline-block;
  font-size: 24rpx;
  padding: 10rpx 20rpx;
  color: #666;
  background-color: #eee;
  border-radius: 20rpx;
  margin: 10rpx 10rpx;
}

button {
  background: transparent;
  border-radius: 0;
  /* line-height: 100rpx; */
  line-height: 1.5;
}

button::after {
  display: none;
}

button .bd {
  font-size: 44rpx;
}

button.icon {
  padding: 0 30rpx;
}

button.text {
  flex: 1;
  background: #00ae66;
  color: #fff;
}

button.active {
  color: #31c27c;
}

button .bd-icon-share {
  color: #31c27c;
}

button .bd-like {
  color: #7a7e83;
}

button.active .bd-like {
  color: #31c27c;
}

.bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: #fff;
  height: 100rpx;
  line-height: 100rpx;
  z-index: 20;
}

.bottom button {
  line-height: 100rpx;
}

.flex-row {
  display: flex;
  flex-direction: row;
}

.bottom::after {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.icon {
  width: 100rpx;
  color: #999;
}

.btn {
  width: 200rpx;
  text-align: center;
  padding: 15rpx 0;
  align-items: center;
  margin: 0 auto;
  font-size: 24rpx;

  color: #fff;
  border-radius: 10rpx;
}

.btn-green {
  background: #31c27c;
}

.btn-orange {
  background: #ff6131;
}

.btn-rounder {
  border: 1px solid #e5e5e5;
  border-radius: 20rpx;
  width: 180rpx;
  text-align: center;
  padding: 10rpx 0;
  align-items: center;
  margin: 0 auto;
  font-size: 24rpx;
}

.btn-rounder .text {
  width: 180rpx;
  text-align: left;
  color: #999;
}

.btn-rounder.active {
  border-color: #31c27c;
}

.btn-rounder.active .icon, .btn-rounder.active .text {
  color: #31c27c;
}

.tips {
  background: #f8f9fc;
  font-size: 28rpx;
  padding: 20rpx;
}

.panel-top {
  background: #fff;
  padding: 20rpx;
  font-weight: bold;
  position: relative;
}

.panel-top::after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1px solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.mt20 {
  margin-top: 20rpx;
}

