.page-user-top {
  height: 370rpx;
  /* overflow: hidden; */
  position: relative;
}

.user-card {
  padding: 0;
  background: #31c27c;
  height: 250rpx;
}

.user-card-info {
  position: absolute;
  left: 40rpx;
  right: 40rpx;
  top: 50rpx;
  display: block;
  background: #fff;
  height: 220rpx;
  box-shadow: 0 4px 25px -3px #e6f4f9;
  z-index: 1;
  border-radius: 20rpx;
  padding: 40rpx;
}

.arrow {
  width: 0;
  height: 0;
  border-radius: 100px;
  border-width: 100px;
  border-style: solid;
  border-color: transparent transparent #31c27c transparent;
  line-height: 99em;
  overflow: hidden;
  cursor: pointer;
  position: absolute;
  top: -90rpx;
  z-index: 0;
}

.arrow.left {
  left: -110rpx;
}

.arrow.right {
  right: -110rpx;
}

.mt20 {
  margin-top: 20rpx;
}

.bg-white {
  background: #fff;
}

.flex-row {
  display: flex;
  flex-direction: row;
}

.between {
  justify-content: space-between;
}

.h1 {
  font-size: 36rpx;
  text-align: center;
  position: relative;
  margin: 20rpx 0;
}

.h1::before {
  content: " ";
  position: absolute;
  bottom: 18rpx;
  left: 0;
  right: 65%;
  border-top: 1px solid #e5e5e5;
  height: 1px;
  color: #e5e5e5;
  margin-left: 40rpx;
}

.h1::after {
  content: " ";
  position: absolute;
  bottom: 18rpx;
  left: 65%;
  right: 0;
  border-bottom: 1px solid #e5e5e5;
  height: 1px;
  color: #e5e5e5;
  margin-right: 40rpx;
}

.panel {
  background: #fff;
  margin: 20rpx 0;
  padding: 40rpx 20rpx;
}

.item {
  padding: 20rpx;
}

.item .label {
  width: 100rpx;
  color: #292929;
}

.item .text {
  font-size: 32rpx;
  color: #292929;
}

.item .text .text-ft {
  font-size: 24rpx;
  color: #87919e;
}

button .bd-like {
  color: #31c27c;
}

button .text {
  font-size: 24rpx;
  color: #87919e;
}

.item .label .bd{color:#87919e;font-size:42rpx;}


button {
  background: transparent;
  border-radius: 0;
  /* line-height: 100rpx; */
  line-height: 1.5;
}

button::after {
  display: none;
}

button .bd {
  font-size: 44rpx;
}

button.icon {
  padding: 0 30rpx;
}

button.text {
  flex: 1;
  background: #00ae66;
  color: #fff;
}

button.active {
  color: #31c27c;
}

button .bd-icon-share {
  color: #31c27c;
}

button .bd-like {
  color: #7a7e83;
}

button.active .bd-like {
  color: #31c27c;
}

.bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: #fff;
  height: 100rpx;
  line-height: 100rpx;
  z-index: 20
}

.bottom button{
  line-height: 100rpx
}

.flex-row {
  display: flex;
  flex-direction: row;
}

.bottom::after {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.description{font-size:28rpx;padding:0 30rpx;line-height: 2}