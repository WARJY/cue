page{background: #fff;}
.flex-row, .flex-column {
  display: flex;
  justify-content: flex-start;
}

.flex-row {
  flex-direction: row;
}

.form-item-upload {
  position: relative;
  font-size: 28rpx;
  color: #999
}

.form-item-upload::after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1px solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.upload-btn {
  display: flex;
  box-sizing: border-box;
  margin: 20rpx;
  height: 210rpx;
  width: 210rpx;
  border: 1rpx solid #d9d9d9;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  background-color: #f9f9f9;

}
.upload-btn image{width:110rpx;height:110rpx;}

.image-plus {
  width: 150rpx;
  height: 150rpx;
  border: 2rpx solid #d9d9d9;
  position: relative;
}

.image-plus-nb {
  border: 0;
}

.image-plus-text {
  color: #888;
  font-size: 28rpx;
}

.image-plus-horizontal {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #d9d9d9;
  width: 4rpx;
  height: 80rpx;
  transform: translate(-50%, -50%);
}

.image-plus-vertical {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #d9d9d9;
  width: 80rpx;
  height: 4rpx;
  transform: translate(-50%, -50%);
}

.page{
  width: 100%;
  height: 70vh;
  display: flex;
  /* justify-content: center;
  align-items: center */
}
.page.page-full{height:86vh;}

.flex{
  display: flex;
  justify-content: center;
  align-items: center
}
.title{
  padding: 20rpx;
  font-size: 28rpx;
}
.img-box{
  width: 210rpx;
  height: 210rpx;
  position: relative;
  margin: 20rpx
}

.img-box .mask{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 20;
  background-color: #666;
  opacity: 0.8;
  display: flex;
  justify-content: center;
  align-items: center
}

.img-box .text{
  font-size: 30rpx;
  color: #fff;
}

.img-box image{
  width: 100%;
  height: 100%
}

/*上传组件 2018-07-07*/
.upload{}
.upload-item{}
