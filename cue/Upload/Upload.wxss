.upload-box{background: #fff;width:750rpx;}
.upload-img {text-align: center;padding:40rpx 0;position: relative}
.upload-img .mask,.uploading-img .mask{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 20;
  background-color: rgba(0, 0, 0, 0.5);
 color:#fff;
  display: flex;
  justify-content: center;
  align-items: center
}
.upload-img image{width:455rpx;height:307rpx;margin:0 auto;}
.upload-desc{padding:20rpx;font-size:28rpx;color:#999}
.uploading-img image{width:750rpx;height:307rpx;margin:0 auto;}

.uploading-img{text-align: center; position: relative}