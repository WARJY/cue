.tabnav_bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 130rpx;
  background: #F7F7FA;
  text-align: center;
  color:#7A7E83;
}

.tabnav_bottom::after {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid #e5e5e5;
    color: #e5e5e5;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.tabNav-image{width:54rpx;height:54rpx;margin:0 auto;}
.tabnav_bottom .flex-1{padding:5rpx 0;}
.tabnav_bottom .flex-1.active{color:#31365c}