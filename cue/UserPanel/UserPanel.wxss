page {
  background: #f4f4f4;
  overflow-x: hidden;
}

.page-user-top {
  height: 450rpx;
  background: #fff;
  overflow: hidden;
  position: relative;
}

.user-card {
  padding: 0;
  background: #31c27c;
  height: 330rpx;
}

.user-card-info {
  position: absolute;
  left: 40rpx;
  right: 40rpx;
  top: 50rpx;
  display: block;
  background: #fff;
  height: 290rpx;
  box-shadow: 0 4px 25px -3px #e6f4f9;
  z-index: 1;
  border-radius: 20rpx;
  padding: 40rpx;
  text-align: center;
}

.user-card-info .image {
  margin-right: 20rpx;
}

.user-card-info .text {
  text-align: left;
}

.user-card-info .image image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
}

.btn-rounder {
  border: 1px solid #31c27c;
  font-size: 28rpx;
  padding: 10rpx 0;
  margin: 10rpx 0;
  color: #31c27c;
  border-radius: 10rpx;
}

button::after {
  border: none;
}

.arrow {
  width: 0;
  height: 0;
  border-radius: 100px;
  border-width: 100px;
  border-style: solid;
  border-color: transparent transparent #31c27c transparent;
  line-height: 99em;
  overflow: hidden;
  cursor: pointer;
  position: absolute;
  top: -40rpx;
  z-index: 0;
}

.arrow.left {
  left: -110rpx;
}

.arrow.right {
  right: -110rpx;
}

.mt20 {
  margin-top: 20rpx;
}

.bg-white {
  background: #fff;
}

.flex-row {
  display: flex;
  flex-direction: row;
}

.between {
  justify-content: space-between;
}

.user-info {
  font-size: 28rpx;
  color: #6e6e6e;
}

.user-info .label {
  margin-right: 20rpx;
}

.user-info .item {
  overflow: hidden;
}

.item.row-2 {
  width: 445rpx;
  text-align: left;
    overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
    max-height: 80rpx;
  -webkit-line-clamp: 2;
}

.icon {
  position: absolute;
  right: 20rpx;
  font-size: 32rpx;
  display: block;
  padding: 20rpx;
  bottom: 20rpx;
}

.f24 {
  font-size: 24rpx;
  color: #6e6e6e;
}

.f28 {
  font-size: 28rpx;
  color: #6e6e6e;
}

.mt10 {
  margin-top: 10rpx;
}
