.flex-row{display: flex;justify-content: space-between}
.scroll-view-item, .modal-title {
  min-height: 100rpx;
  line-height: 100rpx;
  position: relative;
  padding:0 20rpx;
  color:#686868;
}

.scroll-view-item::after, .modal-title::after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1px solid #eee;
  color: #eee;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.scroll-view-item.active {
  color: #22b573;
}
 