.flex {
  display: flex;
  justify-content: center;
  align-items: top;
}
.flex-center {
  display: flex;
  justify-content: center;
  align-items: center;
}

.flex-item {
  flex: 1;
}

.chat .date {
  font-size: 28rpx;
  color: #fff;
  background-color: #ddd;
  padding: 10rpx 20rpx;
  margin-top: 20rpx;
  border-radius: 20rpx;
}
 
.chat .chat-item {
  padding: 10rpx 30rpx;
  justify-content: flex-start;

}

.chat .chat-item .user {
  width: 76rpx;
  height: 76rpx;
  border-radius: 5rpx;
}

.chat .chat-item .content {
  max-width: 500rpx;
  box-sizing: border-box;
  background-color: #fff;
  font-size: 28rpx;
  color: #999;
  margin: 50rpx 0 0 0;
  padding: 20rpx;
  border-radius: 10rpx;
}

.chat .chat-item .content.left {
  margin: 0rpx 100rpx 0 20rpx;
}

.chat .chat-item .content.right {
  margin: 0rpx 20rpx 0 100rpx;
  text-align: left;
  background: #9eea6a;
  color:#000;
}

.comment {
  font-size: 28rpx;
  padding: 0 20rpx;
}

.comment .count {
  color: blue;
}

.comment-item {
  margin-top: 20rpx;
  align-items: flex-start;
  border-bottom: solid 1rpx #eee;
  padding-bottom: 20rpx;
}

.comment-item .user {
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
  margin-top: 10rpx;
}

.comment-item .icon {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
}

.comment-item .name {
  width: 100%;
  font-size: 30rpx;
}

.comment-item .date {
  color: #999;
}

.comment-item .content {
  color: #999;
  margin-top: 10rpx;
}

.comment-panel {
  width: 100%;
  box-sizing: border-box;
  height: 100rpx;
  background-color: #f8f8f8;
  position: fixed;
  bottom: 0;
  box-shadow: 0 0 10rpx #e4e4e4;
  padding: 0 20rpx;
  z-index: 999;
}

.comment-panel .icon {
  width: 40rpx;
  height: 40rpx;
  margin: 0 20rpx;
}

.comment-panel .input {
  font-size: 30rpx;
  padding-left: 20rpx;
  border-radius: 20rpx;
  border: solid 1rpx #eee;
  height: 50rpx;
  line-height: 50rpx;
  margin-right: 20rpx;
  color: #999;
  background: #ffffff
}

.comment-panel .commit {
  font-size: 24rpx;
  margin-left: 20rpx;
  padding: 10rpx 40rpx;
  background-color: #00ae66;
  border-radius: 20rpx;
  color: #fff;
}

.comment-input {
  width: 100%;
  background-color: #fff;
  padding: 20rpx;
  box-sizing: border-box;
  position: fixed;
  top: 0rpx;
  z-index: 100;
  height: 100vh;
}

.comment-input textarea {
  width: auto;
  box-sizing: border-box;
  border: solid 1rpx #eee;
  border-radius: 20rpx;
  padding: 20rpx;
  font-size: 28rpx;
  height: 200rpx;
  margin: 0;
}

.comment-input .commit-box {
  width: 100%;
  text-align: right;
  margin: 20rpx 0;
}

.comment-input .commit {
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  background-color: #00ae66;
  font-size: 28rpx;
  border-radius: 20rpx;
  text-align: center;
  display: inline-block;
}

.quick-reply .title {
  font-size: 30rpx;
  color: #000;
}

.quick-reply .reply-box {
  margin: 20rpx 0;
  width: 100%;
}

.reply-box .item {
  display: inline-block;
  font-size: 24rpx;
  padding: 10rpx 20rpx;
  color: #666;
  background-color: #eee;
  border-radius: 20rpx;
  margin: 10rpx 10rpx;
}

.page {
 
  width: 100%;
  /* height: calc(100vh - 250rpx); */
  background-color: #f5f5f5;
  color: #333;
  padding-bottom:110rpx;
}

.comment-panel .icon {
  width: 40rpx;
  height: 40rpx;
  margin: 0 40rpx;
}

.comment-panel input {
  font-size: 30rpx;
  padding-left: 20rpx;
  border-radius: 10rpx;
  border: solid 1rpx #e6e6e6;
  height: 75rpx;
  background: #fff;
}

.comment-panel .button {
  font-size: 30rpx;
  border-radius: 10rpx;
  border: solid 1rpx #eee;
  height: 65rpx;
  line-height: 65rpx;
  background-color: #31c27c;
  color: #fff;
  padding: 5rpx 40rpx;
  margin-left: 20rpx;
}
.bubble{vertical-align: top;
position: relative;}
.bubble:before, .bubble:after {
position: absolute;
top: 14px;
border: 6px solid transparent;
content: " ";
}
.bubble.right:after {
border-left-color: #9eea6a;
border-left-width: 10px;
right: -16px;
}
.bubble.right {
background-color: #9eea6a;
 
}


.bubble.left {
background-color: #fff;
}
.bubble.left:before {
border-right-color: #fff;
border-right-width: 10px;
left: -16px;
}

.loading-more{text-align: center;font-size:28rpx;height:48rpx;line-height: 48rpx; z-index: 999}

.loading-more .bd{font-size:24rpx;}