.flex-row, .flex-column {
  display: flex;
  justify-content: space-around;
  text-align: center;
}

.flex-column {
  flex-direction: column;
}

.flex-row {
  flex-direction: row;
  position: relative;
}

.text {
  font-size: 24rpx;
  margin-top: 10rpx;
  color: #7a7e83;
}

.image {
  font-size: 52rpx;
  margin: 10rpx 0 0rpx 0;
}

.flex-row::before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #d9d9d9;
  color: #d9d9d9;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
