page {
  background: #fff;
  color: #fff;
}

.flex-row, .flex-column {
  display: flex;
  justify-content: space-between;
}

.flex-column {
  flex-direction: column;
}

.none {
  display: none;
}

.text-center {
  text-align: center;
}

.container {
  position: relative;
}

.flex-row {
  display: flex;
}

.flex-1 {
  flex-grow: 1;
}

.flex-2 {
  flex-grow: 2;
}

.w100 {
  width: 100rpx;
  font-size: 24rpx;
  text-align: center;
}

.text-center {
  text-align: center;
  justify-content: center;
}

.play-btn-panel {
  color: #666;
  margin-top: 22px;
  position: fixed;
  bottom: 0rpx;
  background: #fff;
  left: 0;
  right: 0;
  height: 330rpx;
  line-height: 330rpx;
}

button {
  position: absolute;
  top: 100rpx;
  background: none;
  color: #666;
}

button::after {
  display: none;
}

button:hover {
  background: none;
}
