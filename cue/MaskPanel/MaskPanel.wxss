.mask-bg {
  background: #000;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 998;
  opacity: 0.9;
}

.mask-con {
  position: fixed;
  z-index: 5000;
  width: 690rpx;
  min-height: 260rpx;
  height: 350rpx;
  top: 50%;
  left: 50%;
  margin-left: -345rpx;
  margin-top: -445rpx;
  background: #202123;
  text-align: center;
  border-radius: 20px;
  color: #fff;
 
}

 

.flex-column {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.mask-top {
  margin-bottom: 30rpx;
}

.mask-middle {
  margin: 30rpx;
}

.mask-bottom {
  margin-top: 40rpx;
}

.mask-close {
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  color: #909091;
  font-size: 32rpx;
}

.mask-close i {
  font-size: 48rpx;
}
